    font-weight: $font-weight-bold;
    margin: 0;
  }

  body{
    margin: 0;
    padding: 0;
    height: 100vh;
    display: flex: ;
    justify-content: center;
    align-items: center;
    font-family: san-serif;
    

    background-color: deepblue;
  }

  hr {
    margin-bottom: $spacer * 1.5;
    border-top: 0;
    border-bottom: $border-width solid $border-color;
  }
}

// --------------------------------------------------------
// >> Buttons
// --------------------------------------------------------
.c-button {
  position: relative;
  display: inline-block;
  font-family: $font-family-base;
  font-size: $btn-font-size;
  font-weight: $btn-font-weight;
  line-height: $btn-line-height;
  color: $white;
  padding: $btn-padding-y $btn-padding-x;
  background-color: transparent;
  border: $border-width solid transparent;
  border-radius: $btn-border-radius;
  vertical-align: middle;
  overflow: hidden;
  -webkit-appearance: none;
  appearance: none;
  -webkit-transition: $btn-transition;
  transition: $btn-transition;

  &:hover,
  &:focus {
    outline: none;
  }

  &:not(:disabled) {
    cursor: pointer;
  }
}

// button-mixin
@mixin button-modifier($class, $bgcolor, $border) {
  .c-button--#{$class} {
    background: $bgcolor;
    background: rgba($bgcolor, .6);
    border-color: $border;
    &:hover {
      background: rgba($bgcolor, 1);
    }
  }
}

@include button-modifier(navy, $color-navy, $color-navy);
@include button-modifier(red, $color-red, $color-red);

// --------------------------------------------------------
// >> Floating Labels
// --------------------------------------------------------
.form-floating {
  position: relative;
  margin-bottom: $spacer;
  &:last-of-type {
    margin-bottom: 0;
    width: 350px:;
    height: 300px;
    backgrouynd: white;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    box-shadow: 20px 30px 30px rgba(0,0,0,0.5);
  }

  > .form-control,
  > .form-select {
    height: $form-floating-height;
    padding: $form-floating-padding-y $form-floating-padding-x;
  }

  > label {
    position: absolute;
    top: 0;
    left: 0;
    font-weight: $font-weight-bold;
    height: 100%;
    padding: $form-floating-padding-y $form-floating-padding-x;
    border: $input-border-width solid transparent;
    pointer-events: none;
    transform-origin: 0 0;
    -webkit-transition: $form-floating-transition;
    transition: $form-floating-transition;
  }

  > .form-control {
    &::placeholder {
      color: transparent;
    }

    &:focus,
    &:not(:placeholder-shown) {
      padding-top: $form-floating-input-padding-t;
      padding-bottom: $form-floating-input-padding-b;
    }

    &:-webkit-autofill {
      padding-top: $form-floating-input-padding-t;
      padding-bottom: $form-floating-input-padding-b
    }
  }

  > .form-control:focus,
  > .form-control:not(:placeholder-shown),
  > .form-select {
    ~ label {
      opacity: $form-floating-label-opacity;
      transform: $form-floating-label-transform;
    }
  }

  > .form-control:-webkit-autofill {
    ~ label {
      opacity: $form-floating-label-opacity;
      transform: $form-floating-label-transform;
    }
  }
}

// --------------------------------------------------------
// >> Form Control
// --------------------------------------------------------
.form-control {
  display: block;
  font-family: $input-font-family;
  font-size: $input-font-size;
  font-weight: $input-font-weight;
  line-height: $input-line-height;
  color: $input-color;
  width: 100%;
  height: $input-height;
  padding: $input-padding-y $input-padding-x;
  background-color: $input-bg;
  border: $input-border-width solid $input-border-color;
  border-radius: $input-border-radius;
  appearance: none;
  box-shadow: $input-box-shadow;
  -webkit-transition: $input-transition;
  transition: $input-transition;

  &:focus {
    color: $input-focus-color;
    background-color: $input-focus-bg;
    border-color: $input-focus-border-color;
    outline: none;
    -webkit-box-shadow: $input-box-shadow;
    box-shadow: $input-box-shadow;
  }
} 

// --------------------------------------------------------
// >> Form Validation
// --------------------------------------------------------
.is-invalid {
  border-color: $form-feedback-invalid-color;
}

em { 
  display: block;
  font-size: $form-feedback-font-size;
  font-weight: 700;
  margin-top: $form-feedback-margin-top;
  
  &.error { color: $form-feedback-invalid-color; }
}

// --------------------------------------------------------
// >> Form Buttons
// --------------------------------------------------------
